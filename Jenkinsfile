#!Groovy

pipeline {
    agent any

    stages {
        stage("build") {
            steps {
                echo 'building it...'
            }
        }

        stage("test") {
            steps {
                echo 'testing it...'
            }
        }

        stage("deploy") {
            steps {
                echo 'deploying it...'
            }
        }
    }
}